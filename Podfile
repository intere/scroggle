# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'Scroggle' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Cartography'

  target 'ScroggleTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'Messages' do
  use_frameworks!

  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Cartography'

end
